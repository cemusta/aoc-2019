{
  "name": "aoc2019-nodejs",
  "version": "1.0.0",
  "description": "simple repo for aoc 2019",
  "main": "src/index.js",
  "scripts": {
    "start": "node ./src",
    "dev": "nodemon ./src",
    "test": "cross-env SILENT_LOGS=true jest",
    "test:logs": "jest",
    "test:changes": "jest -o",
    "lint": "eslint .",
    "coverage": "cross-env SILENT_LOGS=true jest --collectCoverage",
    "validate": "npm-run-all lint test:changes",
    "codecov": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cemusta/aoc-2019"
  },
  "keywords": [
    "aoc",
    "adventofcode"
  ],
  "author": "Cem Usta",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^8.2.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "codecov": "^3.6.5",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.13.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  }
}
